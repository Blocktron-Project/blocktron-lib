{
  "env": {
      "node": true,
      "es6": true
  },
  "rules": {
      "array-bracket-spacing": [
          2,
          "never"
      ],
      "parserOptions": {
          "ecmaVersion": 6,
          "ecmaFeatures": {
              "experimentalObjectRestSpread": true
          }
      },
      "no-var": 2,
      "block-scoped-var": 2,
      "brace-style": [
          2,
          "1tbs"
      ],
      "camelcase": 0,
      "computed-property-spacing": [
          2,
          "never"
      ],
      "curly": 2,
      "eol-last": 2,
      "eqeqeq": [
          2,
          "smart"
      ],
      "max-depth": [
          1,
          3
      ],
      "max-len": [
          1,
          200
      ],
      "max-statements": [
          1,
          30
      ],
      "new-cap": 1,
      "no-extend-native": 0,
      "no-mixed-spaces-and-tabs": 2,
      "no-trailing-spaces": 0,
      "no-unused-vars": 1,
      "no-use-before-define": [
          2,
          "nofunc"
      ],
      "object-curly-spacing": [
          2,
          "never"
      ],
      "quotes": [
          2,
          "single",
          "avoid-escape"
      ],
      "semi": [
          2,
          "always"
      ],
      "keyword-spacing": [
          2,
          {
              "before": true,
              "after": true
          }
      ],
      "space-unary-ops": 2
  }
}